{
  "name": "pino-sentry-transport",
  "version": "1.3.1-alpha.0",
  "description": "Integrates Pino and Sentry.",
  "main": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomer-yechiel/pino-sentry-transport"
  },
  "engines": {
    "node": "> 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "fix": "biome check --fix",
    "check": "biome check",
    "check-exports": "attw --pack .",
    "test": "vitest",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "ci": "npm run build && npm run test && npm run lint:check && npm run type:check && npm run spell:check",
    "prepublishOnly": "npm run ci",
    "changeset": "changeset",
    "local-release": "changeset version && changeset publish"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@biomejs/biome": "1.8.3",
    "@changesets/cli": "^2.27.7",
    "@sentry/node": "^8.7.0",
    "@sentry/types": "^8.7.0",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^20.12.13",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "cspell": "^8.8.3",
    "dprint": "^0.46.1",
    "esbuild": "^0.21.4",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "nyc": "^15.1.0",
    "pino": "^9.1.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.1.1",
    "source-map-support": "^0.5.21",
    "tsup": "^8.2.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@sentry/node": "^7.0.0 || ^8.0.0",
    "pino": "^7.0.0 || ^8.0.0 || ^9.0.0"
  },
  "volta": {
    "node": "18.17.1",
    "npm": "9.6.7"
  },
  "overrides": {
    "semver-regex": "3.1.4"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "pino-abstract-transport": "^1.2.0"
  }
}
